/* 自定义了一个 font-family */
@font-face {
    font-family: "iconfont";
    /* IE9*/
    src:
        /* IE6-IE8 */
        url('data:application/x-font-woff;charset=utf-8;base64,') format('woff')
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

